{
  "name": "node-sdk-test",
  "version": "1.0.0",
  "description": "APIs for managing companies and users as well as creating and modifying content in workspaces.",
  "main": "sdktest.js",
  "scripts": {
    "lint": "eslint lib",
    "test": "mocha --reporter mochawesome --reporter-options o=SdkTestReports,f=NodeSdkTestReport --recursive --timeout 10000"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3"
  },
  "dependencies": {
    "bluescape-auth-library": "https://github.com/Bluescape/auth-library.git#4b0d78c807423c2cd53c52ccbf543f25bbd9f0a8",
    "bluescape-sdk-node": "^1.0.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "fs": "0.0.1-security",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2",
    "moment": "^2.17.1",
    "request": "^2.55.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "commonjs": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "no-underscore-dangle": 0,
      "strict": 0,
      "prefer-rest-params": 0
    }
  }
}
